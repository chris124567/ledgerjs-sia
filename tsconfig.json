{
	"compilerOptions": {
		"declaration": true,
		"declarationMap": true,
		"sourceMap": true,
		"strict": true,
		"noImplicitAny": true,
		"noImplicitThis": true,
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"moduleResolution": "node",
		"downlevelIteration": true,
		"outDir": "lib",
		"lib": [
			"es6"
		]
	},
	"include": [
		"src"
	],
	"exclude": [
		"node_modules"
	]
}